{
    "builds": [
        {"src": "/app.py", "use": "@vercel/python"}
    ],
    "routes": [
        {
            "src": "/api/v1/(.*)", 
            "dest": "/app.py",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": ["https://astrell.net", "https://www.astrell.net"]
            }
        }
    ]
}